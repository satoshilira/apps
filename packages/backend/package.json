{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build --profile pay-chain-dev",
    "deploy": "sst deploy --profile pay-chain-dev --region eu-central-1  --stage tst",
    "remove": "sst remove --profile pay-chain-dev --region eu-central-1 --stage tst",
    "deploy:prd": "sst deploy --profile pay-chain-dev --region eu-south-1  --stage prd",
    "remove:prd": "sst remove --profile pay-chain-dev --region eu-south-1 --stage prd",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "sst": "^2.11.5",
    "aws-cdk-lib": "2.79.1",
    "constructs": "10.1.156",
    "typescript": "^5.0.4",
    "@tsconfig/node16": "^1.0.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
